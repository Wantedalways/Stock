<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qiheshengwu.stock.workbench.dao.ParkDao">

    <select id="selectAll" resultType="com.qiheshengwu.stock.workbench.entity.Park">
        select
            id,
            name,
            code
        from
            tbl_park
        order by
            code
    </select>

</mapper>